{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "provider": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/provider",
      "sourceRoot": "projects/provider/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/provider",
            "index": "projects/provider/src/index.html",
            "main": "projects/provider/src/main.ts",
            "polyfills": "projects/provider/src/polyfills.ts",
            "tsConfig": "projects/provider/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/provider/src/favicon.ico",
              "projects/provider/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "./node_modules/ngx-bootstrap/datepicker/bs-datepicker.css",
              "./node_modules/bootstrap/dist/css/bootstrap.min.css",
              "projects/provider/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/provider/src/environments/environment.ts",
                  "with": "projects/provider/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "provider:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "provider:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "provider:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/provider/src/test.ts",
            "polyfills": "projects/provider/src/polyfills.ts",
            "tsConfig": "projects/provider/tsconfig.spec.json",
            "karmaConfig": "projects/provider/karma.conf.js",
            "assets": [
              "projects/provider/src/favicon.ico",
              "projects/provider/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "./node_modules/ngx-bootstrap/datepicker/bs-datepicker.css",
              "./node_modules/bootstrap/dist/css/bootstrap.min.css",
              "projects/provider/src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/provider/tsconfig.app.json",
              "projects/provider/tsconfig.spec.json",
              "projects/provider/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/provider/e2e/protractor.conf.js",
            "devServerTarget": "provider:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "provider:serve:production"
            }
          }
        }
      }
    },
